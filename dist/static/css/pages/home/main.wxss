
@charset "utf-8";
.homeTitle {
  text-align: center;
  padding: 5% 0;
}
.homeTitle h1 {
  font-size: 36rpx;
}
.banner {
  width: 70%;
  margin: 0 auto;
  height: 150rpx;
  background: #409eff;
  margin-bottom: 20rpx;
}
.banner img {
  width: 100%;
  height: 100%;
}
.menus {
  width: 70%;
  margin: 0 auto;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
  padding: 30rpx;
}
.menus > div {
  width: 50%;
  text-align: center;
  height: 150rpx;
}
.cartogram {
  width: 80vw;
  margin: 0 auto;
  background: #eee;
}
.cartogram .catTop {
  background: #ccc;
  height: 100rpx;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
  -webkit-box-pack: justify;
  -ms-flex-pack: justify;
  justify-content: space-between;
  padding: 0 0%;
}
.cartogram .catTop div {
  width: 50%;
  line-height: 50rpx;
  text-align: center;
  color: #fff;
}
.cartogram .catTop div .dots {
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background: #fff;
  display: inline-block;
  margin-right: 10rpx;
}
.chat {
  background: #eee!important;
  height: auto!important;
}